<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Sequence>
            <Action ID="WaitSeconds" duration="2" name="Stage1WaitSeconds2"/>
            <ForceSuccess>
                <Repeat num_cycles="1">
                    <Action ID="UPOGoToGoalByName" state="gpf2" goal_name="stage_4_pos_final_5" name="Stage1GoToGoal2" action="/Make_Plan"/>
                </Repeat>
            </ForceSuccess>
            <Action ID="WaitSeconds" duration="3" name="WaitSecondsEnd"/>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="CallEmptyService">
            <input_port type="std::string" name="service">ROS service name</input_port>
        </Action>
        <Condition ID="CheckBool">
            <input_port type="bool" name="input">Input boolean value</input_port>
        </Condition>
        <SubTree ID="CheckDoorandEnter">
            <input_port name="action"/>
            <input_port name="detection_action"/>
            <input_port name="door"/>
            <output_port name="out_door"/>
        </SubTree>
        <SubTree ID="CheckFireAndExtinguish">
            <input_port name="actextinguish">Extinguish Action Name</input_port>
            <input_port name="actgoto">GoTo Action Name</input_port>
            <input_port name="fire">Fire waypoint</input_port>
            <output_port name="found_fire">Name of the Fire WP if found</output_port>
        </SubTree>
        <Action ID="CheckKey">
            <input_port type="std::string" name="no_key">Key to determine if key to say 'no' has benn pressed</input_port>
            <input_port type="std::string" name="yes_key">Key to determine if key to say 'yes' has been pressed</input_port>
        </Action>
        <SubTree ID="CheckResetRequest">
            <output_port name="reset_requested"/>
        </SubTree>
        <Condition ID="CompareNumbers">
            <input_port type="std::string" name="comparison_op">Comparison operator. Valid operators are &lt;, >, &lt;=, >=, == and !=</input_port>
