            <radius>0.04267</radius>
            <length>0.07357</length>
          </cylinder>
        </geometry>
      </collision>
      <visual name="top_visual">
        <pose>0 0 -0.0376785 0 0 1.5707</pose>
        <geometry>
          <mesh>
            <uri>model://sensors/velodyne_hdl32/meshes/velodyne_top.dae</uri>
          </mesh>
        </geometry>
      </visual>
      <sensor type="ray" name="lase_scan">
        <pose>0 0 -0.004645 0 0 0</pose>
        <visualize>false</visualize>
        <update_rate>10</update_rate>
        <ray>
          <scan>
            <horizontal>
              <samples>512</samples>
              <resolution>1</resolution>
              <min_angle>-3.14159</min_angle>
              <max_angle>3.14159</max_angle>
            </horizontal>
            <vertical>
              <samples>16</samples>
              <resolution>1</resolution>
              <min_angle>-0.2897247</min_angle>
              <max_angle>0.2897247</max_angle>
            </vertical>
          </scan>
          <range>
            <min>0.8</min>
            <max>120</max>
            <resolution>0.012</resolution>
          </range>
          <noise>
            <type>gaussian</type>
            <mean>0.0</mean>
            <stddev>0.0</stddev>
          </noise>
        </ray>
        <plugin name="gazebo_ros_laser_controller" filename="libgazebo_ros_velodyne_laser.so">
          <robotNamespace>ardrone</robotNamespace> 
          <topicName>os1_cloud_node_points</topicName>
          <frameName>os1_sensor</frameName>
          <min_range>0.8</min_range>
          <max_range>120.0</max_range>
          <gaussianNoise>0.008</gaussianNoise>  
        </plugin>
      </sensor>
    </link>
    <joint name='fixed_velodyne' type='fixed'>
